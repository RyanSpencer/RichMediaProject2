{
  "name": "GatchaGatari",
  "version": "1.0.0",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "description": "Gatcha game based on monogatari",
  "main": "./server/app.js",
  "scripts": {
    "buildLoginBundle": "babel ./client/login ./client/helper --presets react --out-file ./hosted/loginBundle.js",
    "buildAppBundle": "babel ./client/app/maker.js ./client/helper --presets react --out-file ./hosted/bundle.js",
    "buildPayBundle": "babel ./client/app/pay.js ./client/helper -- presets react --out-file ./hosted/payBundle.js",
    "build": "npm run buildLoginBundle && npm run buildAppBundle && npm buildPayBundle",
    "watchLoginBundle": "babel --watch ./client/login ./client/helper --presets react --out-file ./hosted/loginBundle.js",
    "watchAppBundle": "babel --watch ./client/app/maker.js ./client/helper --presets react --out-file ./hosted/bundle.js",
    "watchPayBundle": "babel --watch ./client/app/pay.js ./client/helper -- presets react --out-file ./hosted/payBundle.js",
    "nodemon": "nodemon --watch ./server ./server/app.js",
    "start": "node ./server/app.js",
    "pretest": "eslint ./server --fix",
    "test": "echo \"Tests complete\""
  },
  "keywords": [
    "Node",
    "MVC",
    "Redis",
    "Login"
  ],
  "author": "Ryan Spencer",
  "license": "Apache 2.0",
  "dependencies": {
    "connect-redis": "^3.0.2",
    "csurf": "^1.8.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.13.0",
    "helmet": "^1.3.0",
    "underscore": "^1.8.3",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "mongoose": "^4.6.3",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.1",
    "nodemon": "^1.11.0"
  }
}
